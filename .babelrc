{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "lib": "./lib",
          "models": "./models"
        },
        "cwd": "babelrc"
      }
    ],
    "transform-object-rest-spread",
    "transform-class-properties",
    "syntax-class-properties"
  ]
}